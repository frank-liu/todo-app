{
  "name": "todo-app-backend",
  "version": "1.0.0",
  "description": "Backend server for Todo app with Grafana Web Vitals integration",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "express",
    "typescript",
    "grafana",
    "web-vitals"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "helmet": "^8.0.0",
    "morgan": "^1.10.0",
    "prom-client": "^15.1.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.19.17",
    "concurrently": "^9.2.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=16"
  }
}
